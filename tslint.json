{
  "rules": {
    "class-name": true,
    "forin": true,
    "jsdoc-format": true,
    "label-position": true,
    "no-arg": true,
    "no-conditional-assignment": true,
    "no-construct": true,
    "no-debugger": true,
    "no-empty": true,
    "no-inferrable-types": true,
    "no-internal-module": true,
    "no-shadowed-variable": [true, {"temporalDeadZone": false}],
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-unused-expression": true,
    "no-var-keyword": true,
    "radix": true,
    "semicolon": [
      true,
      "always"
    ],
    "switch-default": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "variable-name": [
      true,
      "check-format",
      "ban-keywords"
    ]
  }
}
